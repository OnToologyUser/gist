@prefix : <https://ontologies.semanticarts.com/o/gistCore#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://ontologies.semanticarts.com/o/gistCore> .

<https://ontologies.semanticarts.com/o/gistCore> rdf:type owl:Ontology ;
                                                  owl:versionIRI <https://ontologies.semanticarts.com/o/gistCoreX.x.x> ;
                                                  rdfs:comment "Gist is a minimalist upper ontology created by Semantic Arts" ;
                                                  rdfs:label "gistCore" ;
                                                  <https://ontologies.semanticarts.com/gist/license> "https://creativecommons.org/licenses/by-sa/3.0/" .

#################################################################
#    Annotation properties
#################################################################

###  http://www.w3.org/2000/01/rdf-schema#comment
rdfs:comment rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#label
rdfs:label rdf:type owl:AnnotationProperty .


###  https://ontologies.semanticarts.com/gist/license
<https://ontologies.semanticarts.com/gist/license> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  https://ontologies.semanticarts.com/gist/about
<https://ontologies.semanticarts.com/gist/about> rdf:type owl:ObjectProperty ;
                                                 owl:inverseOf <https://ontologies.semanticarts.com/gist/describedIn> ;
                                                 rdfs:domain <https://ontologies.semanticarts.com/gist/Content> ;
                                                 rdfs:comment "Subject matter of a document." ;
                                                 rdfs:label "About" .


###  https://ontologies.semanticarts.com/gist/accepts
<https://ontologies.semanticarts.com/gist/accepts> rdf:type owl:ObjectProperty ;
                                                   rdfs:comment "The types of input messages that will be allowed" ;
                                                   rdfs:label "Accepts" .


###  https://ontologies.semanticarts.com/gist/actual
<https://ontologies.semanticarts.com/gist/actual> rdf:type owl:ObjectProperty ;
                                                  rdfs:range <https://ontologies.semanticarts.com/gist/TimeInstant> ;
                                                  rdfs:comment "When something did occur, therefore noting an historical event." ;
                                                  rdfs:label "Actual" .


###  https://ontologies.semanticarts.com/gist/actualEnd
<https://ontologies.semanticarts.com/gist/actualEnd> rdf:type owl:ObjectProperty ;
                                                     rdfs:subPropertyOf <https://ontologies.semanticarts.com/gist/actual> ,
                                                                        <https://ontologies.semanticarts.com/gist/end> ;
                                                     rdfs:comment "When something did end, therefore noting an historical event." ;
                                                     rdfs:label "Actual End" .


###  https://ontologies.semanticarts.com/gist/actualStart
<https://ontologies.semanticarts.com/gist/actualStart> rdf:type owl:ObjectProperty ;
                                                       rdfs:subPropertyOf <https://ontologies.semanticarts.com/gist/actual> ,
                                                                          <https://ontologies.semanticarts.com/gist/start> ;
                                                       rdfs:comment "When something did start, therefore noting an historical event." ;
                                                       rdfs:label "Actual Start" .


###  https://ontologies.semanticarts.com/gist/affectedBy
<https://ontologies.semanticarts.com/gist/affectedBy> rdf:type owl:ObjectProperty ;
                                                      owl:inverseOf <https://ontologies.semanticarts.com/gist/affects> ;
                                                      rdfs:comment "Where the effect came from" ;
                                                      rdfs:label "Affected By" .


###  https://ontologies.semanticarts.com/gist/affects
<https://ontologies.semanticarts.com/gist/affects> rdf:type owl:ObjectProperty ;
                                                   rdfs:comment "the subject has or had or will have an effect on the object" ;
                                                   rdfs:label "Affects" .


###  https://ontologies.semanticarts.com/gist/allocatedBy
<https://ontologies.semanticarts.com/gist/allocatedBy> rdf:type owl:ObjectProperty ;
                                                       rdfs:range [ rdf:type owl:Class ;
                                                                    owl:unionOf ( <https://ontologies.semanticarts.com/gist/IntellectualProperty>
                                                                                  <https://ontologies.semanticarts.com/gist/Organization>
                                                                                  <https://ontologies.semanticarts.com/gist/Person>
                                                                                )
                                                                  ] ;
                                                       rdfs:comment "Connection between an ID and the thing that minted the ID.  It may be a person or organization, or could be an algorithm (next available or random number generator)" ;
                                                       rdfs:label "Allocated By" .


###  https://ontologies.semanticarts.com/gist/allows
<https://ontologies.semanticarts.com/gist/allows> rdf:type owl:ObjectProperty ;
                                                  rdfs:domain <https://ontologies.semanticarts.com/gist/Intention> ;
                                                  rdfs:range <https://ontologies.semanticarts.com/gist/Behavior> ;
                                                  rdfs:comment "The intention (say a grant) allows a particular kind of activity (for instance egress)" ;
                                                  rdfs:label "Allows" .


###  https://ontologies.semanticarts.com/gist/aspectOf
<https://ontologies.semanticarts.com/gist/aspectOf> rdf:type owl:ObjectProperty ;
                                                    rdfs:comment "What this aspect is referring to" ;
                                                    rdfs:label "Aspect Of" .


###  https://ontologies.semanticarts.com/gist/basedOn
<https://ontologies.semanticarts.com/gist/basedOn> rdf:type owl:ObjectProperty ;
                                                   owl:inverseOf <https://ontologies.semanticarts.com/gist/basisFor> ;
                                                   rdfs:comment "pointer to the thing something was derived from" ;
                                                   rdfs:label "Based On" .


###  https://ontologies.semanticarts.com/gist/basisFor
<https://ontologies.semanticarts.com/gist/basisFor> rdf:type owl:ObjectProperty ;
                                                    rdfs:comment "Reason for an event" ;
                                                    rdfs:label "Basis For" .


###  https://ontologies.semanticarts.com/gist/categorizedBy
<https://ontologies.semanticarts.com/gist/categorizedBy> rdf:type owl:ObjectProperty ;
                                                         rdfs:comment "Points to a taxonomy item or other less formally defined class." ;
                                                         rdfs:label "Categorized By" .


###  https://ontologies.semanticarts.com/gist/characterizedAs
<https://ontologies.semanticarts.com/gist/characterizedAs> rdf:type owl:ObjectProperty ;
                                                           rdfs:domain <https://ontologies.semanticarts.com/gist/Event> ;
                                                           rdfs:range <https://ontologies.semanticarts.com/gist/Behavior> ;
                                                           rdfs:comment "A way to categorize a behavior." ;
                                                           rdfs:label "Characterized As" .


###  https://ontologies.semanticarts.com/gist/communicationAddressOf
<https://ontologies.semanticarts.com/gist/communicationAddressOf> rdf:type owl:ObjectProperty ;
                                                                  owl:inverseOf <https://ontologies.semanticarts.com/gist/hasCommunicationAddress> ;
                                                                  rdfs:comment "Whose address is this" ;
                                                                  rdfs:label "Communication Address Of" .


###  https://ontologies.semanticarts.com/gist/conformsTo
<https://ontologies.semanticarts.com/gist/conformsTo> rdf:type owl:ObjectProperty ;
                                                      rdfs:range <https://ontologies.semanticarts.com/gist/Intention> ;
                                                      rdfs:comment "The subject conforms to the Object, e.g. meet an obligation, meet terms of an offer, adhere to a specification" ;
                                                      rdfs:label "Conforms To" .


###  https://ontologies.semanticarts.com/gist/connectedTo
<https://ontologies.semanticarts.com/gist/connectedTo> rdf:type owl:ObjectProperty ;
                                                       rdfs:comment "A non-owning, non-causal, non-subordinate (i.e., peer-to-peer) relationship." ;
                                                       rdfs:label "Connected To" .


###  https://ontologies.semanticarts.com/gist/contributesTo
<https://ontologies.semanticarts.com/gist/contributesTo> rdf:type owl:ObjectProperty ;
                                                         rdfs:comment "The parts of a system contribute to the goal/ function of the whole system" ;
                                                         rdfs:label "Contributes To" .


###  https://ontologies.semanticarts.com/gist/denominator
<https://ontologies.semanticarts.com/gist/denominator> rdf:type owl:ObjectProperty ;
                                                       rdfs:domain <https://ontologies.semanticarts.com/gist/RatioUnit> ;
                                                       rdfs:range <https://ontologies.semanticarts.com/gist/UnitOfMeasure> ;
                                                       rdfs:comment "Relates a RatioUnit such as meters/second to the denominator Unit (e.g. second)." ;
                                                       rdfs:label "Denominator" .


###  https://ontologies.semanticarts.com/gist/describedIn
<https://ontologies.semanticarts.com/gist/describedIn> rdf:type owl:ObjectProperty ;
                                                       rdfs:comment "Document the subject matter appeared in" ;
                                                       rdfs:label "Described In" .


###  https://ontologies.semanticarts.com/gist/directPartOf
<https://ontologies.semanticarts.com/gist/directPartOf> rdf:type owl:ObjectProperty ;
                                                        owl:inverseOf <https://ontologies.semanticarts.com/gist/hasDirectPart> ;
                                                        rdfs:comment "The relationship between a  part and a whole where the part has independent existence." ;
                                                        rdfs:label "Direct Part Of" .


###  https://ontologies.semanticarts.com/gist/directSubTaskOf
<https://ontologies.semanticarts.com/gist/directSubTaskOf> rdf:type owl:ObjectProperty ;
                                                           owl:inverseOf <https://ontologies.semanticarts.com/gist/hasDirectSubTask> ;
                                                           rdfs:comment "Immediate parent task" ;
                                                           rdfs:label "Direct Sub Task Of" .


###  https://ontologies.semanticarts.com/gist/directlyPrecededBy
<https://ontologies.semanticarts.com/gist/directlyPrecededBy> rdf:type owl:ObjectProperty ;
                                                              owl:inverseOf <https://ontologies.semanticarts.com/gist/directlyPrecedes> ;
                                                              rdfs:comment "Inverse of directly precedes" ;
                                                              rdfs:label "Directly Preceded By" .


###  https://ontologies.semanticarts.com/gist/directlyPrecedes
<https://ontologies.semanticarts.com/gist/directlyPrecedes> rdf:type owl:ObjectProperty ;
                                                            rdfs:subPropertyOf <https://ontologies.semanticarts.com/gist/precedes> ;
                                                            rdfs:comment "A generic ordering relation indicating that the Subject comes immediately  before the Object." ;
                                                            rdfs:label "Directly Precedes" .


###  https://ontologies.semanticarts.com/gist/directlyRecognizedBy
<https://ontologies.semanticarts.com/gist/directlyRecognizedBy> rdf:type owl:ObjectProperty ;
                                                                rdfs:subPropertyOf <https://ontologies.semanticarts.com/gist/recognizedBy> ;
                                                                rdfs:comment "The party doing the recognition" ;
                                                                rdfs:label "Directly RecognizedBy" .


###  https://ontologies.semanticarts.com/gist/directs
<https://ontologies.semanticarts.com/gist/directs> rdf:type owl:ObjectProperty ;
                                                   rdfs:comment "The set of actuators that a controller can affect" ;
                                                   rdfs:label "Directs" .


###  https://ontologies.semanticarts.com/gist/end
<https://ontologies.semanticarts.com/gist/end> rdf:type owl:ObjectProperty ;
                                               rdfs:range <https://ontologies.semanticarts.com/gist/TimeInstant> ;
                                               rdfs:comment "Connects the subject to its end time." ;
                                               rdfs:label "End" .


###  https://ontologies.semanticarts.com/gist/expressedIn
<https://ontologies.semanticarts.com/gist/expressedIn> rdf:type owl:ObjectProperty ;
                                                       rdfs:comment "The language something was expressed in" ;
                                                       rdfs:label "Expressed In" .


###  https://ontologies.semanticarts.com/gist/fromAgent
<https://ontologies.semanticarts.com/gist/fromAgent> rdf:type owl:ObjectProperty ;
                                                     rdfs:range [ rdf:type owl:Class ;
                                                                  owl:unionOf ( <https://ontologies.semanticarts.com/gist/Address>
                                                                                <https://ontologies.semanticarts.com/gist/Organization>
                                                                                <https://ontologies.semanticarts.com/gist/Person>
                                                                              )
                                                                ] ;
                                                     rdfs:comment "The source of a message or shipment" ;
                                                     rdfs:label "From Agent" .


###  https://ontologies.semanticarts.com/gist/fromPlace
<https://ontologies.semanticarts.com/gist/fromPlace> rdf:type owl:ObjectProperty ;
                                                     rdfs:range <https://ontologies.semanticarts.com/gist/Place> ;
                                                     rdfs:comment "origin" ;
                                                     rdfs:label "From Place" .


###  https://ontologies.semanticarts.com/gist/geoContainedIn
<https://ontologies.semanticarts.com/gist/geoContainedIn> rdf:type owl:ObjectProperty ;
                                                          owl:inverseOf <https://ontologies.semanticarts.com/gist/geoContains> ;
                                                          rdfs:comment "All the transitive places something is located in" ;
                                                          rdfs:label "Geo Contained In" .


###  https://ontologies.semanticarts.com/gist/geoContains
<https://ontologies.semanticarts.com/gist/geoContains> rdf:type owl:ObjectProperty ,
                                                                owl:TransitiveProperty ;
                                                       rdfs:domain [ rdf:type owl:Class ;
                                                                     owl:unionOf ( <https://ontologies.semanticarts.com/gist/GeoPoint>
                                                                                   <https://ontologies.semanticarts.com/gist/GeoRegion>
                                                                                   <https://ontologies.semanticarts.com/gist/GeoRoute>
                                                                                   <https://ontologies.semanticarts.com/gist/GeoSegment>
                                                                                   <https://ontologies.semanticarts.com/gist/GeoVolume>
                                                                                   <https://ontologies.semanticarts.com/gist/Landmark>
                                                                                   <https://ontologies.semanticarts.com/gist/Room>
                                                                                 )
                                                                   ] ;
                                                       rdfs:range [ rdf:type owl:Class ;
                                                                    owl:unionOf ( <https://ontologies.semanticarts.com/gist/GeoPoint>
                                                                                  <https://ontologies.semanticarts.com/gist/GeoRegion>
                                                                                  <https://ontologies.semanticarts.com/gist/GeoRoute>
                                                                                  <https://ontologies.semanticarts.com/gist/GeoSegment>
                                                                                  <https://ontologies.semanticarts.com/gist/GeoVolume>
                                                                                  <https://ontologies.semanticarts.com/gist/Landmark>
                                                                                  <https://ontologies.semanticarts.com/gist/Room>
                                                                                )
                                                                  ] ;
                                                       rdfs:comment "Transitive version of geoDirectlyContains" ;
                                                       rdfs:label "Geo Contains" .


###  https://ontologies.semanticarts.com/gist/geoDirectlyContainedIn
<https://ontologies.semanticarts.com/gist/geoDirectlyContainedIn> rdf:type owl:ObjectProperty ;
                                                                  owl:inverseOf <https://ontologies.semanticarts.com/gist/geoDirectlyContains> ;
                                                                  rdfs:comment "the neighborhood is in the city" ;
                                                                  rdfs:label "Geo Directly Contained In" .


###  https://ontologies.semanticarts.com/gist/geoDirectlyContains
<https://ontologies.semanticarts.com/gist/geoDirectlyContains> rdf:type owl:ObjectProperty ;
                                                               rdfs:subPropertyOf <https://ontologies.semanticarts.com/gist/geoContains> ;
                                                               rdfs:comment "The subject geospatially contains the object.  E.g. the area of a city contains the area of its neighborhoods" ;
                                                               rdfs:label "Geo Directly Contains" .


###  https://ontologies.semanticarts.com/gist/geoOccupiedBy
<https://ontologies.semanticarts.com/gist/geoOccupiedBy> rdf:type owl:ObjectProperty ;
                                                         owl:inverseOf <https://ontologies.semanticarts.com/gist/geoOccupies> ;
                                                         rdfs:comment "what is in the location" ;
                                                         rdfs:label "Geo Occupied By" .


###  https://ontologies.semanticarts.com/gist/geoOccupies
<https://ontologies.semanticarts.com/gist/geoOccupies> rdf:type owl:ObjectProperty ;
                                                       rdfs:domain [ rdf:type owl:Class ;
                                                                     owl:unionOf ( <https://ontologies.semanticarts.com/gist/PhysicalIdentifiableItem>
                                                                                   <https://ontologies.semanticarts.com/gist/PhysicalSubstance>
                                                                                 )
                                                                   ] ;
                                                       rdfs:range [ rdf:type owl:Class ;
                                                                    owl:unionOf ( <https://ontologies.semanticarts.com/gist/Place>
                                                                                )
                                                                  ] ;
                                                       rdfs:comment "A thing occupies are region" ;
                                                       rdfs:label "Geo Occupies" .


###  https://ontologies.semanticarts.com/gist/getter
<https://ontologies.semanticarts.com/gist/getter> rdf:type owl:ObjectProperty ;
                                                  rdfs:subPropertyOf <https://ontologies.semanticarts.com/gist/hasParty> ;
                                                  owl:propertyDisjointWith <https://ontologies.semanticarts.com/gist/giver> ;
                                                  rdfs:comment "The recipient" ;
                                                  rdfs:label "Getter" .


###  https://ontologies.semanticarts.com/gist/giver
<https://ontologies.semanticarts.com/gist/giver> rdf:type owl:ObjectProperty ;
                                                 rdfs:subPropertyOf <https://ontologies.semanticarts.com/gist/hasParty> ;
                                                 rdfs:comment "The active party, the one with the obligation or the one initiating the transfer" ;
                                                 rdfs:label "Giver" .


###  https://ontologies.semanticarts.com/gist/governedBy
<https://ontologies.semanticarts.com/gist/governedBy> rdf:type owl:ObjectProperty ;
                                                      owl:inverseOf <https://ontologies.semanticarts.com/gist/governs> ;
                                                      rdfs:comment "A reference from the thing being governed to the governor" ;
                                                      rdfs:label "Governed By" .


###  https://ontologies.semanticarts.com/gist/governs
<https://ontologies.semanticarts.com/gist/governs> rdf:type owl:ObjectProperty ;
                                                   rdfs:domain [ rdf:type owl:Class ;
                                                                 owl:unionOf ( <https://ontologies.semanticarts.com/gist/Intention>
                                                                               <https://ontologies.semanticarts.com/gist/Organization>
                                                                               <https://ontologies.semanticarts.com/gist/Person>
                                                                               <https://ontologies.semanticarts.com/gist/Template>
                                                                             )
                                                               ] ;
                                                   rdfs:range [ rdf:type owl:Class ;
                                                                owl:unionOf ( <https://ontologies.semanticarts.com/gist/Category>
                                                                              <https://ontologies.semanticarts.com/gist/Content>
                                                                              <https://ontologies.semanticarts.com/gist/GeoRegion>
                                                                              <https://ontologies.semanticarts.com/gist/IntellectualProperty>
                                                                              <https://ontologies.semanticarts.com/gist/Intention>
                                                                              <https://ontologies.semanticarts.com/gist/Organization>
                                                                              <https://ontologies.semanticarts.com/gist/Person>
                                                                              <https://ontologies.semanticarts.com/gist/PhysicalIdentifiableItem>
                                                                              <https://ontologies.semanticarts.com/gist/PhysicalSubstance>
                                                                            )
                                                              ] ;
                                                   rdfs:comment "The subject controls or inhibits the object in some way" ;
                                                   rdfs:label "Governs" .


###  https://ontologies.semanticarts.com/gist/hasAltitude
<https://ontologies.semanticarts.com/gist/hasAltitude> rdf:type owl:ObjectProperty ;
                                                       rdfs:domain <https://ontologies.semanticarts.com/gist/GeoPoint> ;
                                                       rdfs:range <https://ontologies.semanticarts.com/gist/Extent> ;
                                                       rdfs:comment "Distance above sea level" ;
                                                       rdfs:label "Has Altitude" .


###  https://ontologies.semanticarts.com/gist/hasBaseUnit
<https://ontologies.semanticarts.com/gist/hasBaseUnit> rdf:type owl:ObjectProperty ;
                                                       rdfs:subPropertyOf <https://ontologies.semanticarts.com/gist/hasStandardUnit> ;
                                                       rdfs:domain <https://ontologies.semanticarts.com/gist/UnitOfMeasure> ;
                                                       rdfs:range <https://ontologies.semanticarts.com/gist/BaseUnit> ;
                                                       rdfs:comment "EXAMPLE: saying that a furlong hasBaseUnit  meter says it is a DistanceUnit." ,
                                                                    "Relates a UnitOfMeasure to its BaseUnit.  This indicates what kind Unit something is, e.g. saying that a furlong hasBaseUnit  meter says it is a DistanceUnit." ;
                                                       rdfs:label "Has Base Unit" .


###  https://ontologies.semanticarts.com/gist/hasBirthDate
<https://ontologies.semanticarts.com/gist/hasBirthDate> rdf:type owl:ObjectProperty ;
                                                        rdfs:subPropertyOf <https://ontologies.semanticarts.com/gist/actualStart> ;
                                                        rdfs:domain <https://ontologies.semanticarts.com/gist/LivingThing> ;
                                                        rdfs:range <https://ontologies.semanticarts.com/gist/TimeInstant> ;
                                                        rdfs:comment "Date a living thing was \"born\" (or germinated, for plants)." ;
                                                        rdfs:label "Has Birthdate" .


###  https://ontologies.semanticarts.com/gist/hasCommunicationAddress
<https://ontologies.semanticarts.com/gist/hasCommunicationAddress> rdf:type owl:ObjectProperty ;
                                                                   rdfs:range <https://ontologies.semanticarts.com/gist/Address> ;
                                                                   rdfs:comment "Points to a general class of places you can send messages including postal addresses, fax numbers, phone numbers, email, web site, etc." ;
                                                                   rdfs:label "Has Communication Address" .


###  https://ontologies.semanticarts.com/gist/hasDeathDate
<https://ontologies.semanticarts.com/gist/hasDeathDate> rdf:type owl:ObjectProperty ;
                                                        rdfs:subPropertyOf <https://ontologies.semanticarts.com/gist/actualEnd> ;
                                                        rdfs:domain <https://ontologies.semanticarts.com/gist/LivingThing> ;
                                                        rdfs:range <https://ontologies.semanticarts.com/gist/TimeInstant> ;
                                                        rdfs:comment "Date a living thing died" ;
                                                        rdfs:label "Has Death Date" .


###  https://ontologies.semanticarts.com/gist/hasDirectPart
<https://ontologies.semanticarts.com/gist/hasDirectPart> rdf:type owl:ObjectProperty ;
                                                         rdfs:subPropertyOf <https://ontologies.semanticarts.com/gist/hasPart> ;
                                                         rdfs:comment "NOTE: No cascading delete." ,
                                                                      "NOTE: Use this property to directly associate parts. hasPart is the transitive version." ,
                                                                      "The relationship between a whole and a part where the part has independent existence." ;
                                                         rdfs:label "Has Direct Part" .


###  https://ontologies.semanticarts.com/gist/hasDirectSubCategory
<https://ontologies.semanticarts.com/gist/hasDirectSubCategory> rdf:type owl:ObjectProperty ;
                                                                rdfs:comment "Note: Unlike its superproperty gist:hasSubCategory, this property is not transitive. It is essentially the same as the non-transitive skos:narrower, using gist:Category rather than skos:Concept." ,
                                                                             "The subject category is a supercategory of the object category. This property defines the direct links in a category hierarchy; no intermediate categories can exist between the direct links." ;
                                                                rdfs:label "has direct subcategory" .


###  https://ontologies.semanticarts.com/gist/hasDirectSubTask
<https://ontologies.semanticarts.com/gist/hasDirectSubTask> rdf:type owl:ObjectProperty ;
                                                            rdfs:subPropertyOf <https://ontologies.semanticarts.com/gist/hasSubTask> ;
                                                            rdfs:domain <https://ontologies.semanticarts.com/gist/Task> ;
                                                            rdfs:range <https://ontologies.semanticarts.com/gist/Task> ;
                                                            rdfs:comment "Immediate child task" ;
                                                            rdfs:label "Has Direct Sub Task" .


###  https://ontologies.semanticarts.com/gist/hasDirectSuperCategory
<https://ontologies.semanticarts.com/gist/hasDirectSuperCategory> rdf:type owl:ObjectProperty ;
                                                                  rdfs:subPropertyOf <https://ontologies.semanticarts.com/gist/hasSuperCategory> ;
                                                                  rdfs:comment "Note: Unlike its superproperty gist:hasSuperCategory, this property is not transitive. It is essentially the same as the non-transitive skos:broader, using gist:Category rather than skos:Concept." ,
                                                                               "The subject category is a subcategory of the object category. This property defines the direct links in a category hierarchy; no intermediate categories can exist between the direct links." ;
                                                                  rdfs:label "has direct supercategory" .


###  https://ontologies.semanticarts.com/gist/hasFromNode
<https://ontologies.semanticarts.com/gist/hasFromNode> rdf:type owl:ObjectProperty ;
                                                       rdfs:subPropertyOf <https://ontologies.semanticarts.com/gist/networkConnection> ;
                                                       rdfs:comment "The connections at the abstract level of a network.  Note this is directed but the parent is the undirected version" ;
                                                       rdfs:label "Has From Node" .


###  https://ontologies.semanticarts.com/gist/hasGoal
<https://ontologies.semanticarts.com/gist/hasGoal> rdf:type owl:ObjectProperty ;
                                                   rdfs:comment "The reason for doing something" ;
                                                   rdfs:label "Has Goal" .


###  https://ontologies.semanticarts.com/gist/hasIncumbent
<https://ontologies.semanticarts.com/gist/hasIncumbent> rdf:type owl:ObjectProperty ;
                                                        rdfs:comment "What equipment or person is currently in this node.  Note to create a temporal view make a TemporalRelation for this property" ;
                                                        rdfs:label "Has Incumbent" .


###  https://ontologies.semanticarts.com/gist/hasJurisdiction
<https://ontologies.semanticarts.com/gist/hasJurisdiction> rdf:type owl:ObjectProperty ;
                                                           rdfs:comment "when laws and contracts are meted out" ;
                                                           rdfs:label "Has Jurisdiction" .


###  https://ontologies.semanticarts.com/gist/hasMagnitude
<https://ontologies.semanticarts.com/gist/hasMagnitude> rdf:type owl:ObjectProperty ;
